{
  "name": "@effect-workos/workos",
  "version": "0.1.0",
  "devDependencies": {
    "@effect/platform": "^0.94.1",
    "@effect/platform-node": "^0.104.0",
    "effect": "^3.19.14",
    "jose": "^6.1.3"
  },
  "exports": {
    "./*": {
      "types": "./dist/src/*.d.ts",
      "default": "./dist/src/*.js"
    }
  },
  "peerDependencies": {
    "@effect/platform": "^0.94.1",
    "effect": "^3.19.14",
    "jose": "^6.1.3"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf .turbo dist",
    "dev": "tsc --watch --preserveWatchOutput",
    "lint": "eslint .",
    "test": "vitest",
    "test:integration": "vitest --config ./vitest.integration.config.ts",
    "test:once": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "workspaces": [
    "apps/*",
    "libs/*",
    "packages/*"
  ]
}
