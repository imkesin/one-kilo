{
  "name": "@one-kilo/server",
  "version": "0.1.0",
  "dependencies": {
    "@effect/auth-workos": "workspace:*",
    "@effect/experimental": "^0.58.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@one-kilo/domain": "workspace:*",
    "@one-kilo/lib": "workspace:*",
    "@one-kilo/server-api": "workspace:*",
    "@one-kilo/sql": "workspace:*",
    "effect": "^3.19.15"
  },
  "keywords": [],
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "tsc --build tsconfig.src.json",
    "clean": "rm -rf .turbo dist",
    "dev": "pnpm /dev:.+/",
    "dev:node": "pnpm node:base --watch dist/src/dev.js",
    "dev:tsc": "tsc --build tsconfig.src.json --watch --preserveWatchOutput",
    "docker:build": "docker build -f Dockerfile -t one-kilo-server ../..",
    "lint": "eslint .",
    "node:base": "node --env-file=.env.local --enable-source-maps",
    "start": "pnpm node:base dist/src/main.js",
    "test": "vitest",
    "test:integration": "vitest --config ./vitest.integration.config.ts",
    "test:once": "vitest run",
    "typecheck": "tsc --build"
  },
  "type": "module"
}
